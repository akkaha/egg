<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cc.akkaha.egg.db.client.PriceExtraMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cc.akkaha.egg.db.model.PriceExtra">
        <id column="id" property="id" />
        <result column="day" property="day" />
        <result column="box_weight" property="boxWeight" />
        <result column="created_at" property="createdAt" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, day, box_weight AS boxWeight, created_at AS createdAt
    </sql>

</mapper>
